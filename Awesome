#include <iostream>
#include <string>
using namespace std;
     
    class AwesomeClass{
         public:
    AwesomeClass(string z){
     setName (z);
}
             void setName(string x){
        name = x;
    }
       string getName(){
        return name;
    }
         private:
             string name;
};

int main()
{
    AwesomeClass ac("Lucky PhoePhoe \n");
    cout << ac.getName ();
    
    AwesomeClass ac2("sally mcSalad");
    cout << ac2.getName();
    return 0;
}
